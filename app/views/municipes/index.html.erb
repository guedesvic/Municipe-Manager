<p id="notice"><%= notice %></p>

<h1>Municipes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Cns</th>
      <th>Email</th>
      <th>Data nascimento</th>
      <th>Telefone</th>
      <th>Status</th>
      <th>Mail subscription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @municipes.each do |municipe| %>
      <tr>
        <td><%= municipe.nome %></td>
        <td><%= municipe.cpf %></td>
        <td><%= municipe.cns %></td>
        <td><%= municipe.email %></td>
        <td><%= municipe.data_nascimento %></td>
        <td><%= municipe.telefone %></td>
        <td><%= municipe.status %></td>
        <td><%= municipe.receber_emails %></td>
        <td><%= link_to 'Show', municipe %></td>
        <td><%= link_to 'Edit', edit_municipe_path(municipe) %></td>
        <td><%= link_to 'Destroy', municipe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Municipe', new_municipe_path %>
